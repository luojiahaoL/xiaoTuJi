.classif{
  font-size: 30rpx; 
  overflow: hidden;
  color: #747474; 
  position: relative;
}
.list{
  width: 20%;
  display: flex; 
  flex-direction: column;
  align-items: center;
  float: left;
  margin: 20rpx 0;
}
.list>image{
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 5rpx;
}
#kf{
  width: 150rpx;
  height: 120rpx;   
  z-index: 100;
  margin: 0;
  padding: 0;
  position: absolute;
  top: 20rpx;
  left: 0;
  background: none;
}